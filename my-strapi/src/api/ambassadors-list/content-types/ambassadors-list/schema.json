{
  "kind": "collectionType",
  "collectionName": "ambassadors_lists",
  "info": {
    "singularName": "ambassadors-list",
    "pluralName": "ambassadors-lists",
    "displayName": "AmbassadorsList",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "time_zone": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::time-zone.time-zone",
      "inversedBy": "ambassadors"
    },
    "country": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::country.country",
      "inversedBy": "ambassadors"
    },
    "Video": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files",
        "videos"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Languages": {
      "type": "text",
      "default": "English"
    },
    "FullDescription": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "SocialLinks": {
      "displayName": "SocialsLinks",
      "type": "component",
      "repeatable": true,
      "component": "general.socials-links"
    },
    "Gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Famale"
      ]
    },
    "slug": {
      "type": "string",
      "required": true
    }
  }
}
