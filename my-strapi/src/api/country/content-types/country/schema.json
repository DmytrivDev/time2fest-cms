{
  "kind": "collectionType",
  "collectionName": "countries",
  "info": {
    "singularName": "country",
    "pluralName": "countries",
    "displayName": "Country",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "CountryName": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "CountryCode": {
      "type": "string",
      "required": true,
      "regex": "^[A-Z]{2}$"
    },
    "CountryDesc": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "time_zones": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::time-zone.time-zone",
      "inversedBy": "countries"
    },
    "ambassadors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ambassadors-list.ambassadors-list",
      "mappedBy": "country"
    },
    "TimezoneDetail": {
      "displayName": "TimezoneDetail",
      "type": "component",
      "repeatable": true,
      "component": "general.timezone-detail"
    },
    "ShortDesc": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "Background": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Gallery": {
      "displayName": "Gallery",
      "type": "component",
      "repeatable": false,
      "component": "country.gallery"
    },
    "CountrySec": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
